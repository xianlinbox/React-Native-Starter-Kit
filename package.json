{
  "name": "PropertyFinder",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "run-ios": "react-native run-ios",
    "run-android": "react-native run-android",
    "test": "ava src/**/__tests__/*Specs.js --verbose",
    "watch:test": "npm run test -- --watch",
    "cover": "nyc npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "bundle-iOS": "react-native bundle --platform ios --entry-file index.ios.js --bundle-output ./ios/index.ios.bundle --assets-dest ./release --dev false",
    "flow": "flow check",
    "lint": "eslint ./src",
    "storybook": "storybook start -p 7007"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "./test/helpers/setTestEnv.js",
      "react-native-mock/mock"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "exclude": [
      "**/__tests__/**",
      "**/styles/*.js",
      "test/**",
      "node_modules/**"
    ]
  },
  "dependencies": {
    "lodash": "^4.16.2",
    "react": "15.2.0",
    "react-native": "0.30.0",
    "react-native-code-push": "^1.14.1-beta",
    "react-native-router-flux": "^3.35.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-actions": "^0.12.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "@kadira/react-native-storybook": "^2.0.0",
    "appium": "^1.5.3",
    "ava": "^0.16.0",
    "babel-core": "^6.14.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-react-native": "^1.9.0",
    "babel-register": "^6.14.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.6.1",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "express": "^4.14.0",
    "flow-bin": "^0.27.0",
    "getstorybook": "^1.4.5",
    "jsdom": "^9.5.0",
    "jsx-test-helpers": "^1.1.0",
    "nyc": "^8.3.0",
    "proxyquire": "^1.7.10",
    "react-addons-test-utils": "15.2.0",
    "react-dom": "15.2.0",
    "react-native-mock": "^0.2.6",
    "sinon": "^1.17.5",
    "wd": "^0.4.0",
    "ws": "^1.1.1"
  }
}
